{
  "exclude": "(^vendor/)",
  "linters": {
    "golint": {
      "type": "golint",
      "include": "(\\.go$)"
    },
    "govet": {
      "type": "govet",
      "flags": ["-composites=false"],
      "include": "(\\.go$)"
    },
    "gofmt": {
      "type": "gofmt",
      "include": "(\\.go$)"
    }
  }
}
